{{!-- Required Elements --}}

{{~#with url}}
    <meta property="og:image" content="{{this}}">
{{/with~}}

{{!-- Optional Elements --}}

{{~#with secureUrl}}
    <meta property="og:image:secure_url" content="{{this}}">
{{/with}}

{{~#with type}}
    <meta property="og:image:type" content="{{this}}">
{{/with}}

{{~#with width}}
    <meta property="og:image:width" content="{{this}}">
{{/with}}

{{~#with height}}
    <meta property="og:image:height" content="{{this}}">
{{/with}}
