{{#if ampEnabled}}
  <amp-sticky-ad layout="nodisplay">
    <amp-ad data-block-on-consent width="{{ampWidth}}"
            height="{{ampHeight}}"
            type="doubleclick"
            data-slot="{{slot}}"
            data-multi-size="{{ampSizes}}"
            data-multi-size-validation="{{ampMultiSizeValidation}}">
    </amp-ad>
  </amp-sticky-ad>
{{/if}}
