{{#if items}}
<bsp-listcarousel class="PageListJ" data-module data-column-count={{#if listColumns234}}{{listColumns234}}{{else}}3{{/if}}{{include "includes/PageList-attributes.hbs" this}}>
    {{include "includes/PageList-header.hbs" this}}

    {{#with items}}
        <div class="PageListJ-items">
            {{#each this}}
                <div class="PageListJ-items-item">
                    {{#eq ../../listColumns234 "2"}}
                        {{#eq this._template "/page/promo/PagePromo.hbs"}}
                            {{include "/page/promo/PagePromoC.hbs" this media=null}}
                        {{else}}
                            {{render this}}
                        {{/eq}}
                    {{else}}
                        {{#eq this._template "/page/promo/PagePromo.hbs"}}
                            {{include "/page/promo/PagePromoB.hbs" this media=null}}
                        {{else}}
                            {{render this}}
                        {{/eq}}
                    {{/eq}}
                </div>
            {{/each}}
        </div>
    {{/with}}
    {{include "includes/PageList-footer.hbs" this}}
</bsp-listcarousel>
{{/if}}
