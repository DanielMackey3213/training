{{#if items}}
<div class="PageListX" data-module {{include "includes/PageList-attributes.hbs" this}}>
    {{include "includes/PageList-header.hbs" this}}

    {{#with items}}
        <div class="PageListX-items">
            {{#each this}}
                <div class="PageListX-items-item">
                    {{#set imageSize="small-icon"}}
                        {{#eq this._template "/page/promo/PagePromo.hbs"}}
                            {{include "/page/promo/PagePromoA.hbs" this category=null ctaButtonText=../../promoButtonText}}
                        {{else}}
                            {{render this}}
                        {{/eq}}
                    {{/set}}
                </div>
            {{/each}}
        </div>
    {{/with}}

    {{include "includes/PageList-footer.hbs" this}}
</div>
{{/if}}
