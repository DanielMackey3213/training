{{#if items}}
<div class="PageListL" data-module {{include "includes/PageList-attributes.hbs" this}}>
    {{include "includes/PageList-header.hbs" this}}

    {{#with items}}
        <div class="PageListL-items" data-with-borders>
            {{#each this}}
                <div class="PageListL-items-item">
                    {{#eq this._template "/page/promo/PagePromo.hbs"}}
                        {{include "/page/promo/PagePromoB.hbs" this category=null media=null ctaButtonText=../../promoButtonText}}
                    {{else}}
                        {{render this}}
                    {{/eq}}
                </div>
            {{/each}}
        </div>
    {{/with}}

    {{include "includes/PageList-footer.hbs" this}}
</div>
{{/if}}
