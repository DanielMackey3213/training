{{#if items}}
<bsp-listcarousel class="PageListE" data-module data-column-count={{#if listColumns234}}{{listColumns234}}{{else}}3{{/if}}{{include "includes/PageList-attributes.hbs" this}}>
    {{include "includes/PageList-header.hbs" this}}

    {{#with items}}
        <div class="PageListE-items">
            {{#each this}}
                <div class="PageListE-items-item">
                    {{#set lazyLoad=false}}
                        {{#eq ../../listColumns234 "2"}}
                            {{#eq this._template "/page/promo/PagePromo.hbs"}}
                                {{include "/page/promo/PagePromoC.hbs" this imagePositionVertical="top"}}
                            {{else}}
                                {{render this}}
                            {{/eq}}
                        {{else}}
                            {{#eq this._template "/page/promo/PagePromo.hbs"}}
                                {{include "/page/promo/PagePromoB.hbs" this imagePositionVertical="top"}}
                            {{else}}
                                {{render this}}
                            {{/eq}}
                        {{/eq}}
                    {{/set}}
                </div>
            {{/each}}
        </div>
    {{/with}}
    {{include "includes/PageList-footer.hbs" this}}
</bsp-listcarousel>
{{/if}}
