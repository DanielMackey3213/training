{{#if items}}
<bsp-listcarousel class="PageListS" data-module data-width="full" {{include "includes/PageList-attributes.hbs" this}}>
    {{#with items}}
        <div class="PageListS-items">
            {{#each this}}
                <div class="PageListS-items-item">
                    {{#set lazyLoad=false}}
                        {{#eq this._template "/page/promo/PagePromo.hbs"}}
                            {{include "/page/promo/PagePromoModuleB.hbs" this}}
                        {{else}}
                            {{render this}}
                        {{/eq}}
                    {{/set}}
                </div>
            {{/each}}
        </div>
    {{/with}}
    {{include "includes/PageList-footer.hbs" this}}
</bsp-listcarousel>
{{/if}}
