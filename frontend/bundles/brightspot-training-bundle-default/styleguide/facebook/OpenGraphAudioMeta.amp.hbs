{{!-- Required Elements --}}

{{~#with url}}
    <meta property="og:audio" content="{{this}}">
{{/with~}}

{{!-- Optional Elements --}}

{{~#with secureUrl}}
    <meta property="og:audio:secure_url" content="{{this}}">
{{/with}}

{{~#with type}}
    <meta property="og:audio:type" content="{{this}}">
{{/with}}
